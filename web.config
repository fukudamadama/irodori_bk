<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="PythonHandler" path="*" verb="*" modules="FastCgiModule" scriptProcessor="D:\home\python\python.exe" resourceType="Unspecified" requireAccess="Script"/>
    </handlers>
    <fastCgi>
      <application fullPath="D:\home\python\python.exe" arguments="D:\home\site\wwwroot\startup.py"/>
    </fastCgi>
    <defaultDocument>
      <files>
        <clear/>
        <add value="startup.py"/>
      </files>
    </defaultDocument>
  </system.webServer>
  <appSettings>
    <add key="WSGI_HANDLER" value="main.app"/>
    <add key="PYTHONPATH" value="D:\home\site\wwwroot"/>
  </appSettings>
</configuration>